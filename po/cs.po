# User interface
msgid "Exact match"
msgstr "Přesné znění"

msgid "Hidden files"
msgstr "Skryté soubory"

msgid "Fulltext search"
msgstr "Fulltextové hledání"

msgid "Limit results"
msgstr "Limitovat výsledky na:"

msgid "File type"
msgstr "Typ souboru"

msgid "Folder"
msgstr "Adresář"

msgid "Choose folder"
msgstr "Vybrat adresář"

msgid "Search method"
msgstr "Metoda hledání"

msgid "Save to file"
msgstr "Uložit výsledku do souboru"

# Filter file type
msgid "Documents"
msgstr "Textové soubory"

msgid "Images"
msgstr "Obrázky"

msgid "Music"
msgstr "Zvukové soubory"

msgid "Videos"
msgstr "Videa"

# Column titles
msgid "Preview"
msgstr "Náhled"

msgid "Filename"
msgstr "Název"

msgid "Size"
msgstr "Velikost"

msgid "Location"
msgstr "Umístění"

msgid "Last modified"
msgstr "Čas změny"

# Status messages
msgid "Searching for \"%s\""
msgstr "Hledám \"%s\""

msgid "Searching..."
msgstr "Hledám..."

msgid "No files found for \"%s\"."
msgstr "Nenalezen žádný soubor pro hledaný výraz \"%s\"."

msgid "No files were found."
msgstr "Nebyly nalezeny žádné soubory."

msgid "%s files found for \"%s\"."
msgstr "Nalezeno %s souborů pro hledaný výraz \"%s\"."

msgid "Fatal error, search was aborted."
msgstr "Závažná chyba, hledání bylo přerušeno."

# Search daemon messages
msgid "The search backend doesn't support wildcards."
msgstr "Tato metoda vyhledávání nepodporuje zástupný znak."

msgid "Click here to start the search daemon."
msgstr "Klikněte sem pro spuštění vyhledávací služby."

msgid "The search daemon was started."
msgstr "Vyhledávací služba byla spuštěna."

msgid "The search daemon could not be started."
msgstr "Nemohu spustit vyhledávací službu."
